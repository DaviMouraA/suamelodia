{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Next.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/.bin/next",
            "args": [
                "dev"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "port": 9229
        }
    ]
}